[user]
  name = Rozelin
  email = rozelin.dc@gmail.com
  signingkey = TODO
[commit]
  gpgsign = true
[core]
  editor = code --wait
[init]
  defaultBranch = main
[alias]
  ca = commit --amend
  rb = rebase -i
  push-f = push --force-with-lease
  cp = cherry-pick
  r-h = reset --hard
  r-s = reset --soft
  co = checkout .
  graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %C(blue)%cn %C(red)%d %Creset%s'
[help]
  autocorrect = prompt
